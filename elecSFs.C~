void elecSFs(){

  gStyle->SetOptStat(0);
  
  TFile *TTTT_BCDEF = TFile::Open("TopBrussels/TopTreeAnalysisBase/Calibrations/LeptonSF/ElectronSF/Moriond17/TriggerSF_Run2016BCDEF_v2.root");
  TFile *TTTT_GH = TFile::Open("TopBrussels/TopTreeAnalysisBase/Calibrations/LeptonSF/ElectronSF/Moriond17/TriggerSF_Run2016GH_v2.root");
  TFile *TTTT_HLT = TFile::Open("TopBrussels/TopTreeAnalysisBase/Calibrations/LeptonSF/ElectronSF/SF_HLT_Ele32_eta2p1.root");
  
  std::string fullBCDEF = "Ele32_eta2p1_WPTight_Gsf_swappedAxes";
  std::string fullGH = "Ele32_eta2p1_WPTight_Gsf_swappedAxes";
  std::string fullHLT = "SF";

  const char* histBCDEF = fullBCDEF.c_str();
  const char* histGH = fullGH.c_str();
  const char* histHLT = fullHLT.c_str();
  
  TH2F *BCDEF = (TH2F*)TTTT_BCDEF->Get(histBCDEF);
  TH2F *GH = (TH2F*)TTTT_GH->Get(histGH);
  TH2F *HLT = (TH2F*)TTTT_HLT->Get(histHLT);

  printf("BCDEF && GH { \n");
  for(int i = 1; i < BCDEF->GetNbinsX()+1; i++) {
    if( i == 1){
      printf("if (eta > %.3f && eta <= %.3f){ \n", BCDEF->GetXaxis()->GetBinLowEdge(i), BCDEF->GetXaxis()->GetBinLowEdge(i+1));
    }
    else{
      printf("else if (eta > %.3f && eta <= %.3f){ \n", BCDEF->GetXaxis()->GetBinLowEdge(i), BCDEF->GetXaxis()->GetBinLowEdge(i+1));
    }
    for(int j = 1; j < BCDEF->GetNbinsY()+1; j++){
      if( j == 1){
	printf("    if(pt > %.3f && pt <= %.3f){triggerSFBCDEF = %.12f; triggerSFGH = %.12f;} \n", BCDEF->GetYaxis()->GetBinLowEdge(j), BCDEF->GetYaxis()->GetBinLowEdge(j+1), BCDEF->GetBinContent(i,j), GH->GetBinContent(i,j));
      }
      else{
	printf("    else if(pt > %.3f && pt <= %.3f){triggerSFBCDEF = %.12f; triggerSFGH = %.12f;} \n", BCDEF->GetYaxis()->GetBinLowEdge(j), BCDEF->GetYaxis()->GetBinLowEdge(j+1), BCDEF->GetBinContent(i,j), GH->GetBinContent(i,j));
      }
    }
    printf("};\n");
  }

  printf("HLT { \n");
  for(int i = 1; i < HLT->GetNbinsX()+1; i++) {
    if( i == 1){
      printf("if (eta > %.3f && eta <= %.3f){ \n", HLT->GetXaxis()->GetBinLowEdge(i), HLT->GetXaxis()->GetBinLowEdge(i+1));
    }
    else{
      printf("else if (eta > %.3f && eta <= %.3f){ \n", HLT->GetXaxis()->GetBinLowEdge(i), HLT->GetXaxis()->GetBinLowEdge(i+1));
    }
    for(int j = 1; j < HLT->GetNbinsY()+1; j++){
      if( j == 1){
        printf("    if(pt > %.3f && pt <= %.3f){triggerSFHLT = %.12f;} \n", HLT->GetYaxis()->GetBinLowEdge(j), HLT->GetYaxis()->GetBinLowEdge(j+1), HLT->GetBinContent(i,j));
      }
      else{
        printf("    else if(pt > %.3f && pt <= %.3f){triggerSFHLT = %.12f;} \n", HLT->GetYaxis()->GetBinLowEdge(j), HLT->GetYaxis()->GetBinLowEdge(j+1), HLT->GetBinContent(i,j));
      }
    }
    printf("};\n");
  }

}
